@model DataImport.Web.Features.Shared.IApiServerListViewModel
@{
    ViewBag.Title = "Activity";
}

<h2>@ViewBag.Title</h2>

<div id="activity-feed"></div>

@section scripts
{
    <script type="text/javascript">
        $(function() {
            refreshElement("#activity-feed", '@Url.Action("Activity", new { Model.ApiServerId })', seconds(120));
        });
    </script>
}
