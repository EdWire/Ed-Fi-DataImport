{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Debug",
        "Microsoft.Hosting.Lifetime": "Debug",
        "WebOptimizer": "Debug"
      }
    },
    "Using":  [ "Serilog.Sinks.Console"],
    "WriteTo": {
      "ConsoleSink": {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss,fff}] [{LevelShortName}] [{SourceContext}] {Message} {Exception} {NewLine}"
        }
      }
    }
  }
}
