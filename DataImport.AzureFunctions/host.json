{
  "version": "2.0",
  "functionTimeout": "-1",
  "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        }
    },
  "extensions": {
    "queues": {
      "maxPollingInterval": "00:00:10",
      "visibilityTimeout": "00:00:30",
      "batchSize": 1,
      "maxDequeueCount": 5,
      "newBatchThreshold": 1,
      "messageEncoding": "base64"
    },
    "durableTask": {
      "hubName": "dataimportaf",
      "maxConcurrentActivityFunctions": 10,
      "maxConcurrentOrchestratorFunctions": 1,
      "maxEntityOperationBatchSize": 1
    }
  }
}
